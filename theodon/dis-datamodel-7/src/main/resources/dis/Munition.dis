{
  "package" : "com.phyzicsz.dis7",
  "name" : "Munition",
  "parent" : "root",
  "comment" : "An entity's munition (e.g., bomb, missile) information shall be represented by one or more Munition records. For each type or location of munition, this record shall specify the type, location, quantity and status of munitions that an entity contains. Section 6.2.60 ",
  "attributes" : [ {
    "name" : "munitionType",
    "comment" : " This field shall identify the entity type of the munition. See section 6.2.30.",
    "type" : "EntityType"
  }, {
    "name" : "station",
    "comment" : "the station or launcher to which the munition is assigned. See Annex I",
    "type" : "unsigned int"
  }, {
    "name" : "quantity",
    "comment" : "the quantity remaining of this munition.",
    "type" : "unsigned short"
  }, {
    "name" : "munitionStatus",
    "comment" : " the status of the munition. It shall be represented by an 8-bit enumeration. ",
    "type" : "unsigned byte"
  }, {
    "name" : "padding",
    "comment" : "padding ",
    "type" : "unsigned byte"
  } ]
}