{
  "package" : "com.phyzicsz.dis7",
  "name" : "CollisionPdu",
  "parent" : "EntityInformationFamilyPdu",
  "comment" : "Section 7.2.3 Collisions between entities shall be communicated by issuing a Collision PDU. COMPLETE",
  "attributes" : [ {
    "name" : "issuingEntityID",
    "comment" : "This field shall identify the entity that is issuing the PDU, and shall be represented by an Entity Identifier record (see 6.2.28).",
    "type" : "EntityID",
    "isCollection" : false
  }, {
    "name" : "collidingEntityID",
    "comment" : "This field shall identify the entity that has collided with the issuing entity (see 5.3.3.4). This field shall be represented by an Entity Identifier record (see 6.2.28).",
    "type" : "EntityID",
    "isCollection" : false
  }, {
    "name" : "eventID",
    "comment" : "This field shall contain an identification generated by the issuing simulation application to associate related collision events. This field shall be represented by an Event Identifier record (see 6.2.34).",
    "type" : "EventIdentifier",
    "isCollection" : false
  }, {
    "name" : "collisionType",
    "comment" : "This field shall identify the type of collision. The Collision Type field shall be represented by an 8-bit record of enumerations",
    "type" : "unsigned byte",
    "isCollection" : false
  }, {
    "name" : "pad",
    "comment" : "some padding",
    "type" : "byte",
    "initialValue" : "0",
    "isCollection" : false
  }, {
    "name" : "velocity",
    "comment" : "This field shall contain the velocity (at the time the collision is detected) of the issuing entity. The velocity shall be represented in world coordinates. This field shall be represented by the Linear Velocity Vector record [see 6.2.95 item c)].",
    "type" : "Vector3Float",
    "isCollection" : false
  }, {
    "name" : "mass",
    "comment" : "This field shall contain the mass of the issuing entity, and shall be represented by a 32-bit floating point number representing kilograms.",
    "type" : "float",
    "isCollection" : false
  }, {
    "name" : "location",
    "comment" : "This field shall specify the location of the collision with respect to the entity with which the issuing entity collided. The Location field shall be represented by an Entity Coordinate Vector record [see 6.2.95 item a)].",
    "type" : "Vector3Float",
    "isCollection" : false
  } ]
}