{
  "package" : "com.phyzicsz.dis7",
  "name" : "EntityStatePdu",
  "parent" : "EntityInformationFamilyPdu",
  "comment" : "Represents the postion and state of one entity in the world. Section 7.2.2. COMPLETE",
  "attributes" : [ {
    "name" : "entityID",
    "comment" : "Unique ID for an entity that is tied to this state information",
    "type" : "EntityID"
  }, {
    "name" : "forceId",
    "comment" : "What force this entity is affiliated with, eg red, blue, neutral, etc",
    "type" : "unsigned byte"
  }, {
    "name" : "numberOfVariableParameters",
    "comment" : "How many variable parameters are in the variable length list. In earlier versions of DIS these were known as articulation parameters",
    "type" : "unsigned byte"
  }, {
    "name" : "entityType",
    "comment" : "Describes the type of entity in the world",
    "type" : "EntityType"
  }, {
    "name" : "alternativeEntityType",
    "comment" : "",
    "type" : "EntityType"
  }, {
    "name" : "entityLinearVelocity",
    "comment" : "Describes the speed of the entity in the world",
    "type" : "Vector3Float"
  }, {
    "name" : "entityLocation",
    "comment" : "describes the location of the entity in the world",
    "type" : "Vector3Double"
  }, {
    "name" : "entityOrientation",
    "comment" : "describes the orientation of the entity, in euler angles",
    "type" : "EulerAngles"
  }, {
    "name" : "entityAppearance",
    "comment" : "a series of bit flags that are used to help draw the entity, such as smoking, on fire, etc.",
    "type" : "unsigned int"
  }, {
    "name" : "deadReckoningParameters",
    "comment" : "parameters used for dead reckoning",
    "type" : "DeadReckoningParameters"
  }, {
    "name" : "marking",
    "comment" : "characters that can be used for debugging, or to draw unique strings on the side of entities in the world",
    "type" : "EntityMarking"
  }, {
    "name" : "capabilities",
    "comment" : "a series of bit flags",
    "type" : "unsigned int"
  }, {
    "name" : "variableParameters",
    "comment" : "variable length list of variable parameters. In earlier DIS versions this was articulation parameters.",
    "type" : "VariableParameter"
  } ]
}