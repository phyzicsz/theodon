{
  "package" : "com.phyzicsz.dis7",
  "name" : "CollisionElasticPdu",
  "parent" : "EntityInformationFamilyPdu",
  "comment" : "Information about elastic collisions in a DIS exercise shall be communicated using a Collision-Elastic PDU. Section 7.2.4. COMPLETE",
  "attributes" : [ {
    "name" : "issuingEntityID",
    "comment" : "This field shall identify the entity that is issuing the PDU and shall be represented by an Entity Identifier record (see 6.2.28)",
    "type" : "EntityID"
  }, {
    "name" : "collidingEntityID",
    "comment" : "This field shall identify the entity that has collided with the issuing entity. This field shall be a valid identifier of an entity or server capable of responding to the receipt of this Collision-Elastic PDU. This field shall be represented by an Entity Identifier record (see 6.2.28).",
    "type" : "EntityID"
  }, {
    "name" : "collisionEventID",
    "comment" : "This field shall contain an identification generated by the issuing simulation application to associate related collision events. This field shall be represented by an Event Identifier record (see 6.2.34).",
    "type" : "EventIdentifier"
  }, {
    "name" : "pad",
    "comment" : "some padding",
    "type" : "short",
    "initialValue" : "0"
  }, {
    "name" : "contactVelocity",
    "comment" : "This field shall contain the velocity at the time the collision is detected at the point the collision is detected. The velocity shall be represented in world coordinates. This field shall be represented by the Linear Velocity Vector record [see 6.2.95 item c)]",
    "type" : "Vector3Float"
  }, {
    "name" : "mass",
    "comment" : "This field shall contain the mass of the issuing entity and shall be represented by a 32-bit floating point number representing kilograms",
    "type" : "float"
  }, {
    "name" : "locationOfImpact",
    "comment" : "This field shall specify the location of the collision with respect to the entity with which the issuing entity collided. This field shall be represented by an Entity Coordinate Vector record [see 6.2.95 item a)].",
    "type" : "Vector3Float"
  }, {
    "name" : "collisionIntermediateResultXX",
    "comment" : "These six records represent the six independent components of a positive semi-definite matrix formed by pre-multiplying and post-multiplying the tensor of inertia, by the anti-symmetric matrix generated by the moment arm, and shall be represented by 32-bit floating point numbers (see 5.3.4.4)",
    "type" : "float"
  }, {
    "name" : "collisionIntermediateResultXY",
    "comment" : "tensor values",
    "type" : "float"
  }, {
    "name" : "collisionIntermediateResultXZ",
    "comment" : "tensor values",
    "type" : "float"
  }, {
    "name" : "collisionIntermediateResultYY",
    "comment" : "tensor values",
    "type" : "float"
  }, {
    "name" : "collisionIntermediateResultYZ",
    "comment" : "tensor values",
    "type" : "float"
  }, {
    "name" : "collisionIntermediateResultZZ",
    "comment" : "tensor values",
    "type" : "float"
  }, {
    "name" : "unitSurfaceNormal",
    "comment" : "This record shall represent the normal vector to the surface at the point of collision detection. The surface normal shall be represented in world coordinates. This field shall be represented by an Entity Coordinate Vector record [see 6.2.95 item a)].",
    "type" : "Vector3Float"
  }, {
    "name" : "coefficientOfRestitution",
    "comment" : "This field shall represent the degree to which energy is conserved in a collision and shall be represented by a 32-bit floating point number. In addition, it represents a free parameter by which simulation application developers may “tune” their collision interactions.",
    "type" : "float"
  } ]
}